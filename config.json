{
    "base": {
        "protocol": "http",
        "domain": "127.0.0.1:8080",
        "host": "127.0.0.1",
        "schedule_interval": 30,
        "db_path": "db/ch_files.sqlite",
        "logging_level": "INFO",
        "logging_format": "%(asctime)s %(levelname)s %(filename)s %(message)s",
        "necessary_dirs": ["files", "db"]
    },
    "consts": {
        "secret_key_alpha": ["1", "2", "3", "4", "5", "6", "7", "8", "9", "0", "!", "@", "#", "$", "%", "^", "&", "*", "(", ")", "~", "`", "-", "=", "_", "+", " ", "q", "w", "e", "r", "t", "y", "u", "i", "o", "p", "[", "]", "a", "s", "d", "f", "g", "h", "j", "k", "l", ";", "z", "x", "c", "v", "b", "n", "m", ",", ".", "/", "Q", "W", "E", "R", "T", "Y", "U", "I", "O", "P", "{", "}", "A", "S", "D", "F", "G", "H", "J", "K", "L", ":", "Z", "X", "C", "V", "B", "N", "M", "<", ">", "?"],
        "file_key_alpha": "0123456789abcdefghijklmnopqrstuvwxyz",
        "bad_chars": [" ", "/", "\\", "&", "?", "@", "'", "\"", "(", ")"],
        "default_file_key": "abcdef",
        "max_file_key": "zzzzzz"
    },
    "default_settings": {
        ".no_deserialize": true,
        "theme": "neon",
        "start_page": "get",
        "get": "download_it",
        "copy": "none"
    },
    "settings": {
        "theme": {
            ".no_deserialize": true,
            "neon": {
                "name": "Неоновая",
                "path": "../static/css/themes/neon.css",
                "value": "neon"
            }
        },
        "start_page": {
            ".no_deserialize": true,
            "get": {
                "name": "",
                "link": "/get",
                "value": "get"
            },
            "put": {
                "name": "",
                "link": "/put",
                "value": "put"
            }
        }
    }
}
